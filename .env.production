# Configurações de Produção - Easypanel/Docker
# Copie este arquivo para .env e ajuste os valores

# Banco de Dados
DB_NAME=pornx_db
DB_USER=postgres
DB_PASSWORD=CHANGE_THIS_PASSWORD_IN_PRODUCTION
DB_HOST=postgres
DB_PORT=5432

# Aplicação
NODE_ENV=production
PORT=3000
APP_PORT=3000

# Scraping
SCRAPE_DELAY=2000

# URL do banco (gerada automaticamente)
DATABASE_URL=postgresql://${DB_USER}:${DB_PASSWORD}@${DB_HOST}:${DB_PORT}/${DB_NAME}
